{
	"env": {
		"es2021": true,
		"node": true
	},
	"extends": ["airbnb", "prettier"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"project": "./tsconfig.json"
	},
	"ecmaVersion": 12,
	"sourceType": "script",
	"plugins": ["@typescript-eslint"],
	"rules": {
		"no-underscore-dangle": "off",
		"camelcase": "off",
		"no-unused-vars": "warn",
		"import/extensions": "off",
		"import/prefer-default-export": "off"
	}
}
